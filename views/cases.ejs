<html lang="en">

<head>
    <% include ./partials/head %>
        <% include ./partials/header %>
            <% if(typeof(errormessage) !== 'undefined') {%>
                <span>
                    <%= errormessage %>
                </span>
                <% } %>

                    <article class="panel-group" id="accordion">
                        <h1>Case entry details</h1>
                        <section class="panel panel-default">
                            <header class="panel-heading">
                                <h2 class="panel-title">
                                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseone">Search</a>
                                </h2>
                            </header>
                            <div id="collapseone" class="panel-collapse collapse in">
                                <div class="panel-body row">
                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <select name="search-by" id="search-by">
                                            <option value="log-no">Log number</option>
                                            <option value="lastname">Last name</option>
                                            <option value="referrer-org">Referring organisation</option>
                                            <option value="referring-person">Name of person referring</option>
                                            <option value="dob">Date of birth</option>
                                        </select>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <input type="text" id="search-for" placeholder=" search for...">
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <input type="button" class="search-button" data-search="cases" value="search">
                                    </div>
                                    <div class="col-xs-12 response-links">

                                    </div>
                                </div>
                            </div>
                        </section>
                        <form method="post" action="/cases">
                            <section class="panel panel-default">
                                <header class="panel-heading">
                                    <h2 class="panel-title">
                                        <a data-toggle="collapse" data-parent="#accordion" href="#collapsetwo">Basic details</a>
                                    </h2>
                                </header>
                                <div id="collapsetwo" class="panel-collapse collapse">
                                    <div class="panel-body row">
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <span>Log number</span>
                                            <input id="log-number" type="number" name="lognumber" value="" required>
                                            <input id="case-id" name="caseid" class="hidden">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="log-type">Log type</label>
                                            <select name="logtype" id="log-type" required>
                                                <option value="referral">Referral</option>
                                                <option value="case">Case</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <span>Referral date</span>
                                            <input type="date" name="referraldate" required>

                                            <label for="report-by">Report taken by</label>

                                            <select name="reporttakenby" id="report-by" data-list='users' >
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">

                                            <label for="assignedto">Case assigned to</label>
                                            <select name="assignedto" id="assigned-to" data-list="caseworkers">
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">

                                            <span>Date assigned</span>
                                            <input type="date" name="assigneddate" readonly>
                                        </div>
                                        <!-- referral info -->

                                        <div class="col-xs-12">
                                            <h3>General referral info</h3>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="referral-type">Referral type</label>
                                            <select name="referraltype" id="referral-type" data-list="referral">
                                            </select>
                                            <label for="reforganisation">Referrer organisation</label>
                                            <input type="text" name="reforganisation">
                                            <label for="reffirstname">First name</label>
                                            <input type="text" name="reffirstname">
                                            <label for="referrer-lastname">Last name</label>
                                            <input type="text" name="reflastname">
                                            <label for="referrer-role">Role</label>
                                            <input type="text" name="refrole">
                                            <label for="victim-aware">Does the victim know you are making the report?</label>
                                            <select name="refvictimaware" id="victim-aware">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="refadd1">Address line 1</label>
                                            <input type="text" name="refadd1">
                                            <label for="referrer-add2">Address line 2 </label>
                                            <input type="text" name="refadd2">
                                            <label for="referrer-town">Town </label>
                                            <input type="text" name="reftown" data-list='towns'>
                                            <label for="referrer-post-code">Postcode </label>
                                            <input type="text" name="refpostcode">
                                            <label for="referrer-tel">Tel no.</label>
                                            <input type="text" name="reftel">
                                            <label for="referrer-mobile"> Mobile no.</label>
                                            <input type="text" name="refmob">
                                            <label for="referrer-email"> Email address</label>
                                            <input type="text" name="refemail">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="referrer-best-time">Preferred time of contact? </label>
                                            <input type="text" name="refbesttime">
                                            <label for="referrer-preferred-contact">Preferred type of contact?</label>
                                            <select name="refpreferredcontact" id="">
                                                <option value="phone">Phone</option>
                                                <option value="email">Email</option>
                                            </select>
                                            <label for="refreferrer">How did the referrer hear about us?</label>
                                            <input type="text" name="refreferrer">
                                            <label for="referrer-feedback">Does the referrer want feedback?</label>
                                            <select name="reffeedback" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>

                                            <label for="referrer-permission">Does the referrer have permission?</label>
                                            <select name="refpermmision" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="referrer-other">Other info</label>
                                            <textarea name="refotherinfo" id=""></textarea>
                                            <div>
                                                <div class="add-multiple max-5" data-curr="1" data-prefix="flags">
                                                    <label id="flags" for="flags">Flags</label>
                                                    <select name="flags" id="" data-list='flags'>
                                                    </select>
                                                </div>
                                                <input type="button" class='add-multiple-button' value='Add'>
                                                <input type="button" class="add-multiple-button-remove" value='remove'>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                            <!-- victim info -->
                            <section class="panel panel-default">
                                <header class="panel-heading">
                                    <h2 class="panel-title">
                                        <a data-toggle="collapse" data-parent="#accordion" href="#collapsethree">Victim details</a>
                                    </h2>
                                </header>
                                <div id="collapsethree" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="victim-firstname">First name</label>
                                            <input type="text" name="vicfirstname" required>
                                            <label for="victim-lastname">Last name</label>
                                            <input type="text" name="viclastname" required>
                                            <label for="victim-title">Title</label>
                                            <select name="victitle" data-list='title'></select>

                                            <label for="victim-dob">Date of birth</label>
                                            <input type="date" name="vicdob">

                                            <label for="victim-gender">Gender</label>
                                            <select name="vicgender" id="victim-gender" data-list='gender' >
                                            </select>

                                            <label for="victim-ethnicity">Ethnicity</label>
                                            <select name="vicethic" id="" data-list='ethnic' >
                                            </select>
                                            <label for="victim-nationality">Nationality</label>
                                            <select name="vicnationality" id="" data-list='nationality' >
                                            </select>
                                            <label for="victim-sexuality">Sexuality</label>
                                            <select name="vicsexuality" id="" data-list='sexuality'>
                                            </select>
                                            <label for="victim-rorb">Religion or belief</label>
                                            <select name="vicrorb" id="" data-list='religion'>
                                            </select>
                                            <label for="victim-gender-id">Gender ID</label>
                                            <select name="vicgenderid" id="" data-list='genderid'>
                                            </select>

                                            <label for="victim-age">Age (auto)</label>
                                            <input type="number" name="vicage">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">

                                            <label for="victim-add1">Address line 1</label>
                                            <input type="text" name="vicadd1">
                                            <label for="victim-add2">Address line 2 </label>
                                            <input type="text" name="vicadd2">
                                            <label for="victim-town">Town </label>
                                            <select type="text" name="victown" data-list='towns' required></select>
                                            <label for="victim-post-code">Postcode </label>
                                            <input type="text" name="vicpostcode">
                                            <label for="victim-tenure">Tenure</label>
                                            <select name="victenure" id="" data-list='tenure'>
                                            </select>

                                            <label for="victel">Tel no.</label>
                                            <input type="text" name="victel" required>
                                            <label for="victim-mobile"> Alt no.</label>
                                            <input type="text" name="vicmob">
                                            <label for="victim-email"> Email address</label>
                                            <input type="text" name="vicemail">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="victim-best-time">Preferred time of contact? </label>
                                            <input type="text" name="vicbesttime">
                                            <label for="victim-preferred-contact">Preferred type of contact?</label>
                                            <select name="vicpreferredcontact" id="" data-list='prefer'>
                                            </select>
                                            <label for="victim-pets">Pets?</label>
                                            <select name="vicpets" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>
                                            <label for="victim-ward">Ward (?)</label>
                                            <input type="text" name="vicward">
                                            <label for="victim-disabilities">Disabilities?</label>
                                            <input type="text" name="vicdisabilities">
                                            <label for="vicotherinfo">Other info</label>
                                            <textarea type="text" name="vicotherinfo"></textarea>
                                        </div>
                                        <div class="col-xs-12">
                                            <h3>Others who live with victim</h3>
                                        </div>
                                        <div class="col-xs-12 col-md-6">
                                            <div class=" row add-multiple max-5" data-curr='1' data-prefix="cohab">
                                                <h4>Cohabitor</h4>
                                                <label for="cohabtitle">Title</label>
                                                <select type="text" name="cohabtitle" data-list='title'></select>
                                                <label for="cohabfirstname">First name</label>
                                                <input type="text" name="cohabfirstname">
                                                <label for="cohablastname">Last name</label>
                                                <input type="text" name="cohablastname">
                                                <label for="cohabdob">Date of birth</label>
                                                <input type="date" name="cohabdob">
                                                <label for="cohabrelationship">Relationship</label>
                                                <input type="text" name="cohabrelationship">
                                            </div>

                                            <input type="button" class='add-multiple-button' value='Add'>
                                            <input type="button" class="add-multiple-button-remove" value='remove'>
                                        </div>
                                        <div class="col-xs-12">
                                            <h3>Does the victim have other support? (dynamic ad rows)</h3>
                                        </div>
                                        <div>
                                            <div class="row add-multiple max-3" data-curr='1' data-prefix="othersupport">
                                                <div class="col-xs-12 col-sm-6 col-md-3">
                                                    <h4>Other support</h4>
                                                    <label for="othersupport-permission">Permission to conact?</label>
                                                    <select name="othersupportpermission" id="">
                                                        <option value="t">Yes</option>
                                                        <option value="f">No</option>
                                                    </select>

                                                    <label for="othersupportoganisation">Organisation name</label>
                                                    <input type="text" name="othersupportorganisation">
                                                </div>
                                                <div class="col-xs-12 col-sm-6 col-md-3">
                                                    <label for="othersupporttitle">Title</label>
                                                    <select type="text" name="othersupporttitle" data-list='title'></select>
                                                    <label for="othersupportfirstname">First name</label>
                                                    <input type="text" name="othersupportfirstname">
                                                    <label for="othersupportlastname">Last name</label>
                                                    <input type="text" name="othersupportlastname">
                                                    <label for="othersupportadd1">Address line 1</label>
                                                    <input type="text" name="othersupportadd1">
                                                    <label for="othersupportadd2">Address line 2 </label>
                                                    <input type="text" name="othersupportadd2">
                                                </div>
                                                <div class="col-xs-12 col-sm-6 col-md-3">
                                                    <label for="othersupporttown">Town </label>
                                                    <select type="text" name="othersupporttown" data-list='towns'></select>
                                                    <label for="othersupportpost-code">Postcode </label>
                                                    <input type="text" name="othersupportpost-code">
                                                    <label for="othersupporttel">Tel no.</label>
                                                    <input type="text" name="othersupporttel">
                                                    <label for="othersupportmobile"> Mobile no.</label>
                                                    <input type="text" name="othersupportmob">
                                                </div>
                                                <div class="col-xs-12 col-sm-6 col-md-3">
                                                    <label for="othersupportemail"> Email address</label>
                                                    <input type="text" name="othersupportemail">
                                                    <label for="othersupportbest-time">Preferred time of contact? </label>
                                                    <input type="text" name="othersupportbesttime">
                                                    <label for="othersupportpreferred-contact">Preferred type of contact?</label>
                                                    <select name="othersupportpreferredcontact" id="">
                                                        <option value="phone">Phone</option>
                                                        <option value="email">Email</option>
                                                    </select>
                                                    <label for="othersupportrelationship">Relationship </label>
                                                    <input type="text" name="othersupportrelationship">
                                                </div>
                                            </div>
                                            <input type="button" class='add-multiple-button' value='Add'>
                                            <input type="button" class="add-multiple-button-remove" value='remove'>
                                        </div>
                                    </div>
                            </section>
                            <section class="panel panel-default">
                                <header class="panel-heading">
                                    <h2 class="panel-title">
                                        <a data-toggle="collapse" data-parent="#accordion" href="#collapsefour">Incident details</a>
                                    </h2>
                                </header>
                                <div id="collapsefour" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-6 col-md-3">
                                                <label for="incidentporeported">Has it been reported to the police?</label>
                                                <select name="incidentporeported" id="" required="">
                                                    <option value="t">Yes</option>
                                                    <option value="f">No</option>
                                                </select>
                                                <label for="incidentpowanted">Do they want police involvement?</label>
                                                <select name="incidentpowanted" id="">
                                                    <option value="t">Yes</option>
                                                    <option value="f">No</option>
                                                </select>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-3">
                                                <label for="incidentpolog">Police log number</label>
                                                <input type="text" name="incidentpolog">
                                                <label for="police-date">Date of police report</label>
                                                <input type="date" name="indentpodate">
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-3">
                                                <label for="police-name">Name of officer</label>
                                                <input type="text" name="incidentponame">
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-3">
                                                <label for="police-badge">Badge number</label>
                                                <input type="text" name="incidentpobadge">
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentdate">Date of incident</label>
                                            <input type="date">
                                            <label for="incidenttime">Time of incident</label>
                                            <input type="time">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidenttown">Town where incident occurred</label>
                                            <select name="incidenttown" id="" data-list='towns'>
                                            </select>

                                            <label for="incidenttype">Type of hate crime</label>
                                            <select name="incidenttype" id="" data-list='reason' required>
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentspace">Location space</label>
                                            <select name="incidentspace" data-list='places' id="">
                                            </select>
                                            <label for="incidentform">Form of incident</label>
                                            <select name="incidentform" data-list='form' id="">
                                            </select>

                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentaddress">Address where incident took place</label>
                                            <input type="text" name="incidentadd1">
                                            <input type="text" name="incidentadd2">
                                            <input type="text" name="incidentpostcode" placeholder="postcode">
                                            <label for="incidenthowaffected">How has the incident affected the victim?</label>
                                            <input type="text" name="incidenthowaffected">
                                        </div>
                                        <div class="col-xs-12">
                                            <h3>If the incident took place on public transport</h3>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="">Did it take place on public transport?</label>
                                            <select name="incidenttrans" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>
                                            <label for="trans-town">Town of public transport</label>
                                            <select name="incidenttranstown" id="" data-list='towns'>
                                            </select>
                                            <label for="trans-type">Type of public transport</label>
                                            <select name="incidenttranstype" id="" data-list='transport'>
                                            </select>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">

                                            <label for="trans-service">Number of service</label>
                                            <input type="text" name="incidenttransservicenumber">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="trans-from">Travelling from</label>
                                            <input type="text" name="incidenttransfrom">
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="trans-to">Travelling to</label>
                                            <input type="text" name="incidenttransto">
                                        </div>
                                        <div class="col-xs-12">

                                            <h3>Incident details</h3>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentdescription">Description of incident</label>
                                            <textarea name="incidentdesc" rows="8" cols="100"></textarea>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentinjured">Was anyone injured?</label>
                                            <select name="incidentinjury" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>

                                            <label for="incidentinjury-details">Injury details</label>
                                            <textarea name="incidentinjurydetails" rows="8" cols="100"></textarea>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentproperty">Damage/loss of propery</label>
                                            <select name="incidentproperty" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>
                                            <textarea name="incidentpropertydetail" rows="8" cols="100"></textarea>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="incidentwitnesses">Any witnesses?</label>
                                            <select name="incidentwitness" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>

                                            <label for="incidentwitness-details"></label>
                                            <textarea name="incidentwitnessdetail" rows="8" cols="100"></textarea>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="cctv-coverage">CCTV coverage?</label>
                                            <select name="incidentcctv" id="">
                                                <option value="t">Yes</option>
                                                <option value="f">No</option>
                                            </select>

                                            <label for="cctv-coverage-details">CCTV coverage details</label>
                                            <textarea name="incidentcctvdetail" rows="8" cols="100"></textarea>
                                        </div>
                            </section>
                            <section class="panel panel-default">
                                <header class="panel-heading">
                                    <h2 class="panel-title">
                                        <a data-toggle="collapse" data-parent="#accordion" href="#collapsefive">Offender details (ability to add rows)</a>
                                    </h2>
                                </header>
                                <div id="collapsefive" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="col-xs-12">
                                            <h2>Offenders</h2>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-3">
                                            <label for="offenderno">How many were there?</label>
