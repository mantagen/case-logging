                                                    <input type="text" name="offenderfirstname">
                                                    <label for="offenderlastname">Last name</label>
                                                    <input type="text" name="offenderlastname">
                                                    <label for="offendertitle">Title</label>
                                                    <select name="offendertitle" data-list='title'>
                                                    </select>

                                                    <label for="offenderage">Age (auto)</label>
                                                    <input type="number" name="offenderage">


                                                    <label for="offendergender">Gender</label>
                                                    <select name="offendergender" id="offendergender" data-list='gender'>
                                                    </select>

                                                    <label for="offenderethnicity">Ethnicity</label>
                                                    <select name="offenderethnic" id="" data-list='ethnic'>
                                                    </select>
                                                </div>
                                                <div class="col-xs-12 col-sm-6 col-md-3">
                                                    <label for="offenderadd1">Address line 1</label>
                                                    <input type="text" name="offenderadd1">
                                                    <label for="offenderadd2">Address line 2 </label>
                                                    <input type="text" name="offenderadd2">
                                                    <label for="offendertown">Town </label>
                                                    <select type="text" name="offendertown" data-list='towns'></select>
                                                    <label for="offenderpostcode">Postcode </label>
                                                    <input type="text" name="offenderpostcode">

                                                    <label for="offendertel">Tel no.</label>
                                                    <input type="text" name="offendertel">
                                                    <label for="offendermob"> Mobile no.</label>
                                                    <input type="text" name="offendermob">
                                                    <label for="offenderemail"> Email address</label>
                                                    <input type="text" name="offenderemail">
