                    <script>
                        var i = 0;
                        var maxNum = 0;
                        var multElem;
                        var prefix;
                        var prefixLen;
                        var dataCurr;

                        function addMultiple(elem, num) {
                            i = parseInt(elem.attr('data-curr'));
                            if (i < num) {
                                prefix = elem.attr('data-prefix');
                                prefixLen = prefix.length;
                                if (i > 1) {
                                    prefixLen += 1
                                };
                                elem.clone().insertAfter(elem).attr('data-curr', i + 1);
                                dataCurr = elem.siblings('.add-multiple').last().attr('data-curr');
                                elem.siblings('.add-multiple').last().find('input, select, textarea').each(function() {
                                        console.log($(this).parent().attr('data-curr'));
                                        console.log($(this).attr('name'));
                                        $(this).attr('name', prefix + dataCurr + $(this).attr('name').slice(prefixLen));
                                        //$(this).attr('name', prefix + $(this).parent().attr('data-curr') + $(this).parent().siblings().first().children('input, select, textarea').first().attr('name').slice(prefixLen));
                                    })
                                    // clear input values

                            }
                            //i++;
                        }
                        $('.add-multiple-button').click(function() {

                            multElem = $(this).prev('.add-multiple');
                            if (multElem.hasClass('max-3')) maxNum = 3;
                            if (multElem.hasClass('max-5')) maxNum = 5;
                            addMultiple(multElem, maxNum);
                        })

                        function removeMultiple(elem) {
                            console.log(elem);
                            i = parseInt(elem.attr('data-curr'));
                            console.log(i);
                            if (i > 1) {
                                elem.remove();
                                // clear input values
                            };
                        }
                        $('.add-multiple-button-remove').click(function() {

                            multElem = $(this).prev().prev('.add-multiple');
                            removeMultiple(multElem);
                        })
                    </script>
